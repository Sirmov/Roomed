{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Identity": {
    "SignIn": {
      "RequireConfirmedAccount": true
    },
    "Lockout": {
      "MaxFailedAccessAttempts": 5,
      "DefaultLockoutTimeSpan": "00:10:00"
    },
    "Password": {
      "RequireNonAlphanumeric": true,
      "RequireDigit": true,
      "RequireUppercase": true,
      "RequiredLength": 8
    }
  },
  "AllowedHosts": "*"
}
