{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Identity": {
    "SignIn": {
      "RequireConfirmedAccount": false
    },
    "Lockout": {
      "MaxFailedAccessAttempts": 5,
      "DefaultLockoutTimeSpan": "00:10:00"
    },
    "Password": {
      "RequireNonAlphanumeric": false,
      "RequireDigit": false,
      "RequireUppercase": false,
      "RequiredLength": false
    }
  }
}
